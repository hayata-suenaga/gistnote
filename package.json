{
  "name": "gistnote",
  "displayName": "GistNote",
  "publisher": "HayataSuenaga",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:gists",
    "onCommand:gistNote.createPublicGist",
    "onCommand:gistNote.createSecreteGist"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gistNote.openGist",
        "title": "Open Gist"
      },
      {
        "command": "gistNote.refreshGists",
        "title": "Refresh Gists",
        "icon": "$(refresh)"
      },
      {
        "command": "gistNote.createSecreteGist",
        "title": "Create Secrete Gist",
        "category": "GistNote"
      },
      {
        "command": "gistNote.createPublicGist",
        "title": "Create Public Gist",
        "category": "GistNote"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gist-note",
          "title": "GistNote",
          "icon": "$(book)"
        }
      ]
    },
    "views": {
      "gist-note": [
        {
          "id": "gists",
          "name": "Gists",
          "icon": "$(book)",
          "contextualTitle": "GistNote"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "gistNote.refreshGists",
          "when": "view == gists",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "gistNote.createSecreteGist"
        },
        {
          "command": "gistNote.createPublicGist"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "axios": "^0.27.2"
  }
}
